# Configuración de Producción para VPS
# Copia este archivo a .env en tu VPS

# Google Gemini API
GEMINI_API_KEY=tu_api_key_real_aqui

# Twilio (WhatsApp)
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=tu_auth_token_aqui
TWILIO_PHONE_NUMBER=whatsapp:+14155238886

# Puerto del servidor (Nginx hace proxy a este puerto)
PORT=3001

# Entorno
NODE_ENV=production

# Dominio de producción (opcional, para logs)
PRODUCTION_DOMAIN=https://tu-dominio.com